<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cibertec.webservicerestandroid.ReservaActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:text="Ticket Reserva"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtreserva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_marginStart="45dp"
        android:layout_toEndOf="@+id/textView3"
        android:text="reserva" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="12dp"
        android:text="Pelicula Seleccionada"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtpelicula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtreserva"
        android:layout_alignTop="@+id/textView3"
        android:text="pelicula" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="14dp"
        android:text="Local"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtlocal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtpelicula"
        android:layout_alignTop="@+id/textView5"
        android:text="local" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="14dp"
        android:text="Sala"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtsala"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/txtlocal"
        android:text="sala" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="17dp"
        android:text="Inicio"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtinicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtsala"
        android:layout_alignTop="@+id/textView7"
        android:text="Inicio" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="13dp"
        android:text="Fin"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtfin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtinicio"
        android:layout_alignTop="@+id/textView11"
        android:text="fin" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="22dp"
        android:text="Datos del Cliente"
        android:textAppearance="@android:style/TextAppearance.Holo" />

    <TextView
        android:id="@+id/txtdni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtfin"
        android:layout_alignTop="@+id/textView13"
        android:text="dni" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtcliente"
        android:layout_marginTop="29dp"
        android:layout_toEndOf="@+id/textView13"
        android:text="IMPRIMIR" />

    <TextView
        android:id="@+id/txtcliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtdni"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="12dp"
        android:text="cliente" />
</RelativeLayout>
